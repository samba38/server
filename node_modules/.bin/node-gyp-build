#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/bin.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules/node-gyp-build/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/.pnpm/node-gyp-build@4.8.4/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/node-gyp-build/bin.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS-Part-2/myapp/node_modules/node-gyp-build/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/bin.js" "$@"
else 
  exec node  "$basedir/../node-gyp-build/bin.js" "$@"
fi
